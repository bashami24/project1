 ,CASE WHEN  REOPEN_TIMESTAMP between '2022-10-01'and '2022-10-31 23:59:59'THEN MAX(NEW_REOPENED_REPORTED_COUNT) OVER 
 (PARTITION BY  CLAIM_NUMBER,EXPOSURE_PUBLICID_NK, CLAIM_SYMBOL_CODE,YEAR(REOPEN_TIMESTAMP), MONTH(REOPEN_TIMESTAMP),REOPEN_TIMESTAMP) --ELSE 0 END AS NEW_REOPENED_REPORTED_COUNT_1
 QUALIFY ROW_NUMBER () OVER (PARTITION BY  CLAIM_NUMBER,EXPOSURE_PUBLICID_NK, 
    CLAIM_SYMBOL_CODE order by NEW_REOPENED_REPORTED_COUNT desc) =1 ) ELSE 0 END AS NEW_REOPENED_REPORTED_COUNT_1
