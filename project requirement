,CASE
    WHEN REOPEN_TIMESTAMP BETWEEN '2022-11-01' AND '2022-11-30 23:59:59'
         AND MAX(NEW_REOPENED_REPORTED_COUNT) OVER(PARTITION BY CLAIM_NUMBER, EXPOSURE_PUBLICID_NK, CLAIM_SYMBOL_CODE) = 1
         AND LAG(STATUS_NAME) OVER(PARTITION BY CLAIM_NUMBER, EXPOSURE_PUBLICID_NK, CLAIM_SYMBOL_CODE ORDER BY REOPEN_TIMESTAMP) = 'Closed'
         AND LAG(MONTH(REOPEN_TIMESTAMP)) OVER(PARTITION BY CLAIM_NUMBER, EXPOSURE_PUBLICID_NK, CLAIM_SYMBOL_CODE ORDER BY REOPEN_TIMESTAMP) <> 11
    THEN 1
    ELSE 0
END AS NEW_REOPENED_REPORTED_COUNT_1
