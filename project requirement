// services/ProductService.ts

const apiBaseURL = "http://localhost:9017/";

export function getAllProductsAPI() {
    return fetch(apiBaseURL + "product", {
        method: "GET",
        mode: "cors"
    });
}

export function addProductAPI(productId: number, productName: string, price: number, sellerId: number) {
    return fetch(apiBaseURL + "product", {
        method: "POST",
        mode: "cors",
        headers: { "Content-Type": "application/json" },
        body: JSON.stringify({ id: productId, name: productName, price, sellerId })
    });
}

export function deleteProductAPI(productId: number){
    return fetch (apiBaseURL + `product/${productId}`,{
        method:"DELETE",
        mode: "cors"
    });
}

export const updateProductAPI = async (productId: number, newName: string, newPrice: number, newSellerId: number): Promise<void> => {
    try {
        await fetch(`${apiBaseURL}/products/${productId}`, {
            method: 'PUT',
            headers: {
                'Content-Type': 'application/json'
            },
            body: JSON.stringify({ name: newName, price: newPrice, sellerId: newSellerId })
        });
    } catch (error) {
        console.error('Error updating product:', error);
        throw error;
    }
};


